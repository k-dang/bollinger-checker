{
  "name": "cron-worker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "eslint src && wrangler deploy -e=dev",
    "deploy:prod": "eslint src && wrangler deploy -e=prod",
    "dev": "wrangler dev --test-scheduled",
    "start": "wrangler dev --test-scheduled",
    "cf-typegen": "wrangler types",
    "lint": "eslint src",
    "alpaca": "tsx --env-file=.env testscripts/test-alpaca.ts",
    "fmp": "tsx --env-file=.env src/fmp.ts",
    "yahoo": "tsx --env-file=.env testscripts/test-yahoo.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.13",
    "eslint": "^9.31.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "wrangler": "^4.24.3"
  },
  "dependencies": {
    "trading-signals": "^6.8.0",
    "yahoo-finance2": "^2.13.3"
  }
}
