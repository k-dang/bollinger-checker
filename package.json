{
  "name": "cron-worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "eslint src && wrangler deploy -e=dev",
    "deploy:prod": "eslint src && wrangler deploy -e=prod",
    "dev": "wrangler dev --test-scheduled",
    "start": "wrangler dev --test-scheduled",
    "cf-typegen": "wrangler types",
    "lint": "eslint src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "alpaca": "tsx --env-file=.env testscripts/test-alpaca.ts",
    "yahoo": "tsx --env-file=.env testscripts/test-yahoo.ts",
    "rsi": "tsx --env-file=.env testscripts/test-rsi.ts",
    "providers": "tsx --env-file=.env testscripts/test-providers.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.28.1"
  },
  "dependencies": {
    "trading-signals": "^6.9.1",
    "yahoo-finance2": "^2.13.3"
  }
}
